<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<DialogPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx">

    <content>
        <HBox>
            <Label text="%lobby.create.label.name"/>
            <TextField fx:id="lobbyNameField"/>
        </HBox>
    </content>

    <ButtonType text="%lobby.create.button.ok" buttonData="OK_DONE"/>
    <ButtonType text="%lobby.create.button.cancel" buttonData="CANCEL_CLOSE"/>
</DialogPane>
